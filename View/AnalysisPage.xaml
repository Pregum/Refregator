<Page x:Class="MVVM_Refregator.View.AnalysisPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MVVM_Refregator.View"
      xmlns:vm="clr-namespace:MVVM_Refregator.ViewModel"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NutrientInformation">

    <Page.DataContext>
        <vm:AnalysisPageViewModel />
    </Page.DataContext>

    <Grid>
        <!--<TextBlock Text="栄養素情報ページです"/>-->
        <Button Content="Jsonファイルを読み込みます" Command="{Binding Send_ReadJson}" Margin="10,411,627,10"/>
        <DataGrid ItemsSource="{Binding FoodList}" MinWidth="687" Margin="0,0,0,30" />
    </Grid>
</Page>
